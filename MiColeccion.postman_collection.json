							"pm.test(\"Status code should be 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"email should match\", function () {\r",
							"    pm.expect(jsonData.email).to.be.eq(\"anamaria@hotmail.com\");\r",
